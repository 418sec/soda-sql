      <td>password</td>
      <td>Â </td>
      <td>Required</td>
    </tr>
    <tr>
      <td>account</td>
      <td>Eg YOUR_SNOWFLAKE_ACCOUNT.eu-west-1</td>
      <td>Required</td>
    </tr>
    <tr>
      <td>warehouse</td>
      <td>Â </td>
      <td>Required</td>
    </tr>
